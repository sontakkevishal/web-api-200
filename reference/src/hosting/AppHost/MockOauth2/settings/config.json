{
  "loginPagePath": "/app/resources/software/html/login.html",
  "interactiveLogin": true,
  "httpServer": "MockWebServerWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "software",
      "requestMappings": [
        {
          "requestParam": "code",
          "match": "mary",
          "claims": {
            "sub": "mary",
            "role": [
              "SoftwareCenter",
              "Manger"
            ]
          }
        },
        {
          "requestParam": "code",
          "match": "alice",
          "claims": {
            "sub": "alice"
          }
        },
        {
          "requestParam": "code",
          "match": "earl",
          "claims": {
            "sub": "earl",
            "role": [
              "SoftwareCenter"
            ]
          }
        }
      ]
    }
  ]
}